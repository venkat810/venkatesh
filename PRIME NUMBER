
lower=int(input('enter your lower number: '))
upper=int(input('enter your upper number: '))
for num in range(lower,upper+1):
    if num>1:
        for i in range(2,num):
            if (num%i)==0:
                #print(num,'it is not a prime number')
                break
        else:
            print(num,'it is a prime number')
